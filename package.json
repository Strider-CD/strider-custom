{
  "name": "strider-custom",
  "version": "0.7.0",
  "description": "Support for custom prepare, test and deploy phases in Strider",
  "main": "index.js",
  "scripts": {
    "test": "eslint *.js && mocha -R tap"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/Strider-CD/strider-custom.git"
  },
  "keywords": [
    "strider"
  ],
  "engines": {
    "node": ">=4.2"
  },
  "author": "Niall O'Higgins",
  "license": "MIT",
  "readmeFilename": "README.md",
  "dependencies": {
    "debug": "^2.2.0",
    "ejs": "^1.0.0",
    "gitane": "~0.3.1"
  },
  "strider": {
    "id": "custom",
    "type": "job",
    "icon": "icon.png",
    "title": "Custom Scripts",
    "worker": "worker.js",
    "webapp": "webapp.js",
    "config": true
  },
  "devDependencies": {
    "chai": "~1.8.1",
    "eslint": "^3.1.1",
    "mocha": "~1.14.0"
  }
}
